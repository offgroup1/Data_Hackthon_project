      "source": [
        "#점수 부여하기\n",
        "def score(row): \n",
        "  if row > 10 :\n",
        "    return 4\n",
        "  elif  row > 3 and row  <= 10 :\n",
        "    return 3\n",
        "  elif row == 2 or row == 3:\n",
        "    return 2\n",
        "  elif row ==1:\n",
        "    return 1\n",
        "\n",
        "count2['c_purchase_level'] = count2.c_purchase_count.apply(score)\n"
      ],
      "metadata": {
        "id": "xA0i8HHiCdo2"
      },
      "execution_count": null,
      "outputs": []
    },
